<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:fast="urn:import:com.biker.client.common.fast">
	<ui:style>
	
	</ui:style>
	
	<ui:with field="css" type="com.biker.client.common.resource.CoreStyle"></ui:with>
	
	<g:HTMLPanel>
	
		<fast:FastPress ui:field="org" styleName="nomargin {css.shopContext} {css.shopContextClick}">
			<fast:inner>
				<g:HTMLPanel>
					<g:InlineLabel ui:field="org_name"></g:InlineLabel>
					<i class="fa fa-chevron-down {css.shopChevron}"></i>
				</g:HTMLPanel>
			</fast:inner>
		</fast:FastPress>
		<ul class="nomargin {css.navItems}">
			<li>
				<fast:FastPress ui:field="inventory" styleName="nomargin {css.shopContext} {css.shopContextClick}">
					<fast:inner>
						<g:HTMLPanel>
							<i class="fa fa-calculator"></i>
							<span>Inventory</span>
						</g:HTMLPanel>
					</fast:inner>
				</fast:FastPress>
			</li>
			<li>
				<fast:FastPress ui:field="associates" styleName="nomargin {css.shopContext} {css.shopContextClick}">
					<fast:inner>
						<g:HTMLPanel>
							<i class="fa fa-group"></i>
							<span>Associates</span>
						</g:HTMLPanel>
					</fast:inner>
				</fast:FastPress>
			</li>
			<li>
				<fast:FastPress ui:field="shopSettings" styleName="nomargin {css.shopContext} {css.shopContextClick}">
					<fast:inner>
						<g:HTMLPanel>
							<i class="fa fa-gear"></i>
							<span>Shop Settings</span>
						</g:HTMLPanel>
					</fast:inner>
				</fast:FastPress>
			</li>
		</ul>
	</g:HTMLPanel>
</ui:UiBinder> 